{
  "name": "@ruvector/postgres",
  "version": "0.1.0",
  "description": "Supabase-style CLI for RuVector PostgreSQL vector extension - high-performance similarity search",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "ruvector-pg": "bin/cli.js",
    "rvpg": "bin/cli.js"
  },
  "scripts": {
    "build": "tsc -b",
    "clean": "rm -rf dist *.tsbuildinfo",
    "prepublishOnly": "npm run build",
    "postinstall": "node scripts/postinstall.js || true",
    "test": "node test/cli-test.js",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src --ext .ts"
  },
  "keywords": [
    "postgresql",
    "postgres",
    "vector",
    "database",
    "extension",
    "pgvector",
    "similarity",
    "search",
    "embeddings",
    "hnsw",
    "ivfflat",
    "simd",
    "ruvector",
    "cli"
  ],
  "author": "ruv.io Team <info@ruv.io> (https://ruv.io)",
  "homepage": "https://ruv.io",
  "bugs": {
    "url": "https://github.com/ruvnet/ruvector/issues"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ruvnet/ruvector.git",
    "directory": "npm/packages/postgres-cli"
  },
  "files": [
    "dist",
    "bin",
    "scripts",
    "templates",
    "README.md"
  ],
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "chalk": "^4.1.2",
    "commander": "^11.1.0",
    "conf": "^10.2.0",
    "execa": "^5.1.1",
    "fs-extra": "^11.2.0",
    "inquirer": "^8.2.6",
    "listr2": "^8.0.1",
    "ora": "^5.4.1",
    "semver": "^7.6.0",
    "table": "^6.8.1",
    "yaml": "^2.4.1"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/inquirer": "^9.0.7",
    "@types/node": "^20.10.5",
    "@types/semver": "^7.5.8",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "os": [
    "darwin",
    "linux",
    "win32"
  ],
  "cpu": [
    "x64",
    "arm64"
  ]
}
